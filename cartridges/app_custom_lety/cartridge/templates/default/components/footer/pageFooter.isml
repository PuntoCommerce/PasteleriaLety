<isscript>
    var assets = require('*/cartridge/scripts/assets.js');
    assets.addCss('/css/footer.css');
</isscript>

<footer id="footercontent">
    <div class="container-1450">
        <div class="row">
            <div class="footer-item col-sm-2 store-mobile">
                <div class="footer-email-signup footer-email-signup-mobile" style="padding-left: 1.5em;">
                    <form role="form">
                        <div class="row">
                            <div class="input-group">
                                <div class="suscribe-label col-sm-12 d-flex"
                                    style="padding-bottom: .5em; justify-content: center;">
                                    <span>${Resource.msg('description.form.emailsignup', 'homePage', null)}</span>
                                </div>
                                <div class="d-flex justify-content-start input-button"
                                    style="display: flex; flex-direction:column">

                                    <input type="text" class="form-control col-sm-12 newsletter"
                                        style="margin-bottom: .5em" name="hpEmailSignUp"
                                        placeholder="${Resource.msg('placeholdertext.form.emailsignup', 'homePage', null)}"
                                        aria-label="${Resource.msg('placeholdertext.form.emailsignup', 'homePage', null)}">
                                    <span class="input-group-append col-sm-12" style="margin-bottom: .5em">
                                        <button type="submit" class="subscribe-email "
                                            data-href="${URLUtils.url('EmailSubscribe-Subscribe')}">
                                            ${Resource.msg('button.form.emailsignup', 'homePage', null)}</button>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>

            <div class="footer-item col-sm-3 collapsible-xs">
                <iscontentasset aid="footer-contact" />
            </div>

            <div class="footer-item col-sm-2 collapsible-xs">
                <iscontentasset aid="footer-legal" />
            </div>
            <div class="footer-item col-sm-3 collapsible-xs">
                <iscontentasset aid="footer-nosotros" />
            </div>
            <div class="footer-item footer-item-img col-sm-4" style="margin-left: -3em;">
                <div class="footer-email-signup footer-news-desk" style="padding-left: 1.5em;">
                    <form role="form">
                        <div class="row">
                            <div class="input-group">
                                <div class="suscribe-label d-flex justify-content-start">
                                    <span>${Resource.msg('description.form.emailsignup', 'homePage', null)}</span>
                                </div>
                                <div class="d-flex justify-content-start align-items-center input-button">

                                    <input type="text" class="form-control newsletter" name="hpEmailSignUp"
                                        placeholder="${Resource.msg('placeholdertext.form.emailsignup', 'homePage', null)}"
                                        aria-label="${Resource.msg('placeholdertext.form.emailsignup', 'homePage', null)}">
                                    <span class="input-group-append">
                                        <button type="submit" class="subscribe-email"
                                            data-href="${URLUtils.url('EmailSubscribe-Subscribe')}">
                                            ${Resource.msg('button.form.emailsignup', 'homePage', null)}</button>
                                    </span>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
                <div class="row" style="padding-left: 1.5em;">
                    <div class="col-lg-12 col-sm-5 push-sm-7 push-lg-8 social-h2" style="text-align: left;">
